                        <constraint firstItem="JLY-ur-69a" firstAttribute="width" secondItem="qjd-mM-CEa" secondAttribute="width" multiplier="0.75" id="ony-YD-Run"/>
                    </constraints>
                </view>
                <view contentMode="scaleToFill" id="T7E-ap-8kd">
                    <rect key="frame" x="0.0" y="0.0" width="375" height="667"/>
                    <autoresizingMask key="autoresizingMask" flexibleMaxX="YES" flexibleMaxY="YES"/>
                    <subviews>
                        <label opaque="NO" userInteractionEnabled="NO" contentMode="left" horizontalHuggingPriority="251" verticalHuggingPriority="251" misplaced="YES" textAlignment="natural" lineBreakMode="tailTruncation" numberOfLines="0" baselineAdjustment="alignBaselines" adjustsFontSizeToFit="NO" translatesAutoresizingMaskIntoConstraints="NO" id="lcI-p0-frw">
                            <rect key="frame" x="67" y="151" width="240" height="364"/>
                            <mutableString key="text">The main display has the area, general conditions and current temperature. Under that is an hourly collection showing temperature, conditions, rain chance and time.  Below that is a daily list for the next 8 days. This abbreviated list contains high/low temperatures, precipitation chance, conditions, and sunrise and sunset times.  And finally at the bottom are the current conitions for the area selected.  </mutableString>
                            <fontDescription key="fontDescription" type="system" pointSize="17"/>
                            <nil key="textColor"/>
                            <nil key="highlightedColor"/>
                        </label>
                    </subviews>
                    <color key="backgroundColor" white="0.0" alpha="0.0" colorSpace="calibratedWhite"/>
